{
  "name": "@goodware/log",
  "version": "1.3.0",
  "description": "Batteries-included Winston3-based loggger with console, file, and AWS CloudWatch Logs transports",
  "scripts": {
    "docs": "rm -rf docs ; node_modules/.bin/jsdoc --configure .jsdoc.json --verbose -R README.md *.js",
    "lint": "eslint . --fix",
    "playground": "node tests/playground.js",
    "format": "prettier . --write",
    "pub2": "npm publish --access=public",
    "prepub": "npm test && npm upgrade && npm run format && npm run lint && npm run docs",
    "test": "node tests/test.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/good-ware/js-log"
  },
  "keywords": [
    "winston",
    "logging",
    "cloudwatch-logs"
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "aws-sdk": "^2.869.0",
    "hostid": "^1.0.0",
    "humanize-duration": "^3.0.0",
    "joi": "^17.0.0",
    "json-prune": "^1.0.0",
    "mkdirp-sync": "^0.0.3",
    "uuid": "^8.0.0",
    "winston": "^3.0.0",
    "winston-cloudwatch": "^2.0.0",
    "winston-console-format": "^1.0.0",
    "winston-daily-rotate-file": "^4.0.0"
  },
  "devDependencies": {
    "better-docs": "^2.0.0",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jsdoc": "^3.0.0",
    "prettier": "^2.0.0",
    "request-promise": "^4.0.0",
    "why-is-node-running": "^2.0.0"
  }
}
